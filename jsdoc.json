{
  "name":"i福田",
  "plugins": [
    "plugins/markdown",
    "plugins/summarize"
  ],
  "recurseDepth": 10,
  "source": {
    "include": [
      "./src"
    ],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_",
    "exclude": []
  },
  "tutorial1": {
    "title": "Tutorial One",
    "children": {
      "childA": {
        "title": "Child A"
      },
      "childB": {
        "title": "Child B"
      }
    }
  },
  "tutorial2": {
    "title": "Tutorial Two"
  },
  "sourceType": "module",
  "tags": {
    "recurse": true,
    "destination": "./docs/",
    "allowUnknownTags": true,
    "dictionaries": [
      "jsdoc",
      "closure"
    ]
  },
  "opts": {
    "template": "templates/default",
    "encoding": "utf8",
    "destination": "./docs/",
    "recurse": true,
    "verbose": false
  },
  "templates": {
    "default": {
      "useLongnameInNav": true,
      "includeDate": false
    },
    "cleverLinks": false,
    "monospaceLinks": false
  }
}